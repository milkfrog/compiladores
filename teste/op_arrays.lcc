def divide(int number, int array, int size) {
	int i;
	
	if (number == 0) {
		print "erro";
		return;
	}
	
	for (i = 0; i < size; i = i + 1) {
		array[i] = array[i] / number;
		print "saida: " + array[i];
	}
}

def multiplica(int number, int array, int size)
{
	int i;
	
	for (i = 0; i < size; i = i + 1) {
		array[i] = array[i] * number;
		print "saida: " + array[i];
	}
}