def divide(int number, int array)
{
	int array_len;
	array_len = len(array);
	if (array_len == 0) {
		return;
	}

	if (number == 0) {
		print "erro";
		return;
	}
	
	int i;
	for (i = 0; i < array_len; i = i + 1) {
		array[i] = array[i] / number;
		print "saida: ";
		print array[i];
		print "\n";
	}
	return;
}

def multiplica(int number, int array)
{
	int array_len;
	array_len = len(array);
	if (array_len == 0) {
		return;
	}

	int i;
	for (i = 0; i < array_len; i = i + 1) {
		array[i] = array[i] * number;
		print "saida: ";
		print array[i];
		print "\n";
	}
}

def dobra(int array)
{
	int array_len;
