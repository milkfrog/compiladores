}

def sum(int array_a, int array_b, int array_dest)
{
	int array_a_len;
	array_a_len = len(array_a);
	int array_b_len;
	array_b_len = len(array_b);
	int array_dest_len;
	array_dest_len = len(array_dest);

	if (array_len_a != array_len_b) {
		return;
	}
	if (array_len_a != array_len_dest) {
		return;
	}

	int i;
	for (i = 0; i < array_a_len; i = i + 1) {
		array_dest[i] = array_a[i];
	}

	int j;
	for (j = 0; j < array_b_len; j = j + 1) {
		array_dest[j] = array_b[j] + array_dest[j];
	}

	return;
}

